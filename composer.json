{
  "name": "winardiaris/nusoap",
  "type": "library",
  "description": "Custom NuSOAP for PHP 5.6 - 7.3",
  "keywords": ["soap","nusoap","php","http","xml","transport","client"],
  "license": "LGPL-2.0-only",
  "homepage": "https://github.com/winardiaris/nusoap",
  "authors": [
    {
      "name": "Aris Winardi",
      "homepage": "http://www.winardiaris.xyz/"
    }
  ],
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "~1.0.0"
  },
  "autoload": {
    "classmap": ["src/"]
  },
  "scripts": {
    "linter": [
      "parallel-lint src"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-0.9.x": "0.9.7.x-dev",
      "dev-develop": "1.0.x-dev"
    }
  }
}
